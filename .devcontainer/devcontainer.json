{
    "name": "Python 3",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "backend",
    "workspaceFolder": "/app",
    "postCreateCommand": "echo 'source /workspaces/backend/load_env.sh' >> ~/.bashrc && pip3 install --user -r requirements.txt",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-azuretools.vscode-docker"
            ]
        }
    }
}
